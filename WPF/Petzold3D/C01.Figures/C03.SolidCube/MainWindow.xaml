<Window x:Class="C03.SolidCube.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Solid Cube" Height="400" Width="400">
  <Window.Resources>
    <Style TargetType="Polygon">
      <Setter Property="StrokeStartLineCap" Value="Round"/>
      <Setter Property="StrokeEndLineCap" Value="Round"/>
      <Setter Property="StrokeLineJoin" Value="Round"/>
    </Style>
  </Window.Resources>
  <Canvas>
    <Polygon Points="96 96, 192 96, 192 192, 96 192" Stroke="Black" Fill="White"/>
    <Polygon Points="144 144, 240 144, 240 240, 144, 240" Stroke="Black" Fill="White"/>
    <Polygon Points="96 96, 192 96, 240 144, 144 144" Stroke="Black" Fill="White"/>
    <Polygon Points="96 96, 144 144, 144 240, 96 192" Stroke="Black" Fill="White"/>
  </Canvas>
</Window>
